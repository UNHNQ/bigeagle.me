<script src="<%- config.root %>js/jquery.imagesloaded.min.js"></script>
<script src="<%- config.root %>js/gallery.js"></script>

<% if (config.disqus_shortname){ %>
<script type="text/javascript">

<% if (page.dq_id){ %>
var disqus_identifier = "<%= page.dq_id %>";
<% } else { %>
    <% if (page.wordpress_id) {%>
    var disqus_identifier = "<%= page.wordpress_id%> <%= config.url %>/?p=<%=page.wordpress_id%>";
    <% }else {%>
        var disqus_identifier = "<%= config.url%>/<%= page.path%>";
    <% } %>
<% }%>

    
var disqus_shortname = '<%= config.disqus_shortname %>';

(function(){
  var dsq = document.createElement('script');
  dsq.type = 'text/javascript';
  dsq.async = true;
  dsq.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
  (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
}());
</script>
<% } %>

<% if (theme.fancybox){ %>
<link rel="stylesheet" href="http://bigeagle.qiniudn.com/jquery.fancybox.css" media="screen" type="text/css">
<script src="http://bigeagle.qiniudn.com/jquery.fancybox.pack.js"></script>
<script type="text/javascript">
(function($){
  $('.fancybox').fancybox();
})(jQuery);
</script>
<% } %>

